fn check(x: i32): Result<i32, str> {
    if (x > 0) {
        return Ok(x);
    } else {
        return Err("Negative");
    }
}

fn main() {
    var res = check(10);
    if (res.is_ok()) {
        println("Result is Ok: {res.unwrap()}");
    }
    
    // expect testi
    // check(-1).expect("Should fail"); 
}

