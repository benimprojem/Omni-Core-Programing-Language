fn find(id: i32): Option<str> {
    if (id == 1) {
        return Some("Found");
    }
    return None;
}

fn main() {
    var res = find(1);
    if (res.is_some()) {
        println("Option: {res.unwrap()}");
    }
    
    var none_val = find(99);
    println("Unwrap Or: {none_val.unwrap_or("Default")}");
}
