// TEST 9: String İşlemleri
// Beklenen: String interpolation ve strlen fonksiyonunun çalışması

fn main(): i32 {
    echo("=== TEST 9: STRING İŞLEMLERİ ===\n");
    
    // String literal
    var mesaj: str = "NIMBLE Derleyici";
    echo("Mesaj: {mesaj}\n");
    
    // String interpolation
    var isim: str = "Ahmet";
    var yas: i32 = 25;
    echo("İsim: {isim}, Yaş: {yas}\n");
    
    // strlen fonksiyonu
    var uzunluk: i32 = strlen(mesaj);
    echo("Mesaj uzunluğu: {uzunluk}\n");
    
    // Çoklu interpolation
    var a: i32 = 10;
    var b: i32 = 20;
    var toplam: i32 = a + b;
    echo("{a} + {b} = {toplam}\n");
    
    // Float ile interpolation
    var pi: f64 = 3.14159;
    echo("Pi değeri: {pi}\n");
    
    echo("\n=== TEST 9 TAMAMLANDI ===\n");
    return 0;
}
