// TEST 4: Karşılaştırma Operatörleri
// Beklenen: ==, !=, <, >, <=, >= operatörlerinin doğru çalışması

fn main(): i32 {
    echo("=== TEST 4: KARŞILAŞTIRMA OPERATÖRLER ===\n");
    
    var a: i32 = 10;
    var b: i32 = 20;
    
    echo("a = {a}, b = {b}\n");
    
    // Eşitlik
    if (a == b) {
        echo("a == b: true\n");
    } else {
        echo("a == b: false\n");
    }
    
    // Eşitsizlik
    if (a != b) {
        echo("a != b: true\n");
    } else {
        echo("a != b: false\n");
    }
    
    // Küçüktür
    if (a < b) {
        echo("a < b: true\n");
    } else {
        echo("a < b: false\n");
    }
    
    // Büyüktür
    if (a > b) {
        echo("a > b: true\n");
    } else {
        echo("a > b: false\n");
    }
    
    // Küçük eşit
    if (a <= b) {
        echo("a <= b: true\n");
    } else {
        echo("a <= b: false\n");
    }
    
    // Büyük eşit
    if (a >= b) {
        echo("a >= b: true\n");
    } else {
        echo("a >= b: false\n");
    }
    
    echo("\n=== TEST 4 TAMAMLANDI ===\n");
    return 0;
}
