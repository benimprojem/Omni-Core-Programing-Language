// TEST 10: Array Tanımlama ve Erişim
// Beklenen: Array literal, erişim ve arrlen fonksiyonunun çalışması

fn main(): i32 {
    echo("=== TEST 10: ARRAY TANIMLAMA VE ERİŞİM ===\n");
    
    // Array tanımlama
    var sayilar: arr = [10, 20, 30, 40, 50];
    echo("Array tanımlandı: [10, 20, 30, 40, 50]\n");
    
    // Array uzunluğu
    var uzunluk: i32 = arrlen(sayilar);
    echo("Array uzunluğu: {uzunluk}\n");
    
    // Array elemanlarına erişim
    var ilk: i32 = sayilar[0];
    echo("İlk eleman (sayilar[0]): {ilk}\n");
    
    var ikinci: i32 = sayilar[1];
    echo("İkinci eleman (sayilar[1]): {ikinci}\n");
    
    var son: i32 = sayilar[4];
    echo("Son eleman (sayilar[4]): {son}\n");
    
    // Array elemanı güncelleme
    sayilar[2] = 99;
    var guncellenen: i32 = sayilar[2];
    echo("Güncellenen eleman (sayilar[2]): {guncellenen}\n");
    
    echo("\n=== TEST 10 TAMAMLANDI ===\n");
    return 0;
}
