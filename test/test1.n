// TEST 1: Temel Literaller ve Echo
// Beklenen: Tüm literal tiplerin doğru yazdırılması

fn main(): i32 {
    echo("=== TEST 1: TEMEL LİTERALLER ===\n");
    
    // Integer literal
    echo("Integer: ");
    echo("42\n");
    
    // Float literal
    echo("Float: ");
    echo("3.14\n");
    
    // String literal
    echo("String: ");
    echo("Merhaba Dünya\n");
    
    // Boolean literal
    echo("Boolean True: ");
    echo("true\n");
    
    echo("Boolean False: ");
    echo("false\n");
    
    // Char literal
    echo("Char: ");
    echo("A\n");
    
    echo("\n=== TEST 1 TAMAMLANDI ===\n");
    return 0;
}