// TEST 14: Tip Dönüşümleri
// Beklenen: _int, _float, _str fonksiyonlarının çalışması

fn main(): i32 {
    echo("=== TEST 14: TİP DÖNÜŞÜMLERİ ===\n");
    
    // Float to Int
    var f: f64 = 3.14;
    var i: i32 = _int(f);
    echo("Float to Int: {f} -> {i}\n");
    
    // Int to Float
    var x: i32 = 42;
    var y: f64 = _float(x);
    echo("Int to Float: {x} -> {y}\n");
    
    // Int to String
    var sayi: i32 = 123;
    var str_sayi: str = _str(sayi);
    echo("Int to String: {sayi} -> {str_sayi}\n");
    
    // Float to String
    var pi: f64 = 3.14159;
    var str_pi: str = _str(pi);
    echo("Float to String: {pi} -> {str_pi}\n");
    
    // Char to Int
    var ch: char = 'A';
    var ascii: i32 = _int(ch);
    echo("Char to Int: {ch} -> {ascii}\n");
    
    echo("\n=== TEST 14 TAMAMLANDI ===\n");
    return 0;
}
