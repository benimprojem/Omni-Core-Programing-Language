// TEST 11: For-In Döngüsü (Array)
// Beklenen: Array üzerinde for-in döngüsünün çalışması

fn main(): i32 {
    echo("=== TEST 11: FOR-IN DÖNGÜSÜ (ARRAY) ===\n");
    
    // Integer array üzerinde döngü
    var sayilar: arr = [1, 2, 3, 4, 5];
    echo("Sayılar: ");
    for (sayi in sayilar) {
        echo("{sayi} ");
    }
    echo("arr tipi bitti.\n");
    
    // Toplam hesaplama
    var toplam: i32 = 0;
    for (x in sayilar) {
        toplam = toplam + x;
    }
    echo("Toplam: {toplam}\n");
    
    // Farklı bir array
    var degerler: arr = [10, 20, 30, 40];
    echo("Değerler: ");
    for (deger in degerler) {
        echo("{deger} ");
    }
    echo("\n");
    
    echo("\n=== TEST 11 TAMAMLANDI ===\n");
    return 0;
}
